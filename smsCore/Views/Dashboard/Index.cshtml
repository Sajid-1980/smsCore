@{
    ViewBag.Title = "Home Page";
}
<style SASS>
    .card-columns {
        -moz-column-count: 2;
        -webkit-column-count: 2;
        column-count: 2;
    }

    @@media (max-width: 991px) {
        .card-columns {
            -moz-column-count: 1;
            -webkit-column-count: 1;
            column-count: 1;
        }
    }
</style>
<link href="~/assets/charts/Chart.min.css" rel="stylesheet"/>
<script src="~/assets/charts/Chart.min.js"></script>
<div class="sico-container p-0">

    <div class="row">
        <div class="col-12">

            <div class="py-4" id="sortable">
                <div class="infobox infobox-green infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-building fa-2x"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Classes</div>
                        <div class="infobox-content">0</div>
                    </div>
                </div>

                <div class="infobox infobox-blue infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-users"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Employees</div>
                        <div class="infobox-content">@ViewBag.lblEmployeeTotal</div>
                    </div>
                </div>

                <div class="infobox infobox-green infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-check"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Present</div>
                        <div class="infobox-content">@ViewBag.lblEmployeePresent</div>
                    </div>
                </div>

                <div class="infobox infobox-red infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-adn"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Absent</div>
                        <div class="infobox-content">@ViewBag.lblEmployeeAbsent</div>
                    </div>
                </div>

                <div class="infobox infobox-grey infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa  fa-tachometer"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Leave</div>
                        <div class="infobox-content">@ViewBag.lblEmployeeLeave</div>
                    </div>
                </div>


                <div class="infobox infobox-purple infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa 	fa-briefcase"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Students</div>
                        <div class="infobox-content">@ViewBag.lblStudentTotal</div>
                    </div>
                </div>


                <div class="infobox infobox-green infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-check"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Present</div>
                        <div class="infobox-content">@ViewBag.lblStdPresent</div>
                    </div>
                </div>

                <div class="infobox infobox-red infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-adn"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Absent</div>
                        <div class="infobox-content">@ViewBag.lblStdAbsent</div>
                    </div>
                </div>

                <div class="infobox infobox-grey infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa  fa-tachometer"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Leave</div>
                        <div class="infobox-content">@ViewBag.lblStdLeave</div>
                    </div>
                </div>

                <div class="infobox infobox-grey infobox-small infobox-dark">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-users"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Parents</div>
                        <div class="infobox-content">@ViewBag.lblPrentsTotal</div>
                    </div>
                </div>
                <div class="infobox infobox-green infobox-dark" style="height: 52px;">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-dollar"></i>
                    </div>
                    <div class="infobox-data">
                        <div class="infobox-content">Free Admissions</div>
                        <div class="infobox-content">@ViewBag.FreeAdmission</div>
                    </div>
                </div>
            </div>

            <div class="row">

                <div class="card-columns w-100" id="sortableDivs">
                    <div class="card">
                        <div class="card-header">

                        </div>
                        <div class="card-body">

                            <canvas id="sessionChart" height="150"></canvas>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-header">

                        </div>
                        <div class="card-body">

                            <canvas id="studentChart" height="150"></canvas>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">

                        </div>
                        <div id="classfeegroup-gridContainer" class="card-body">

                            <table id="jqgclassFeeGroup" cellpadding="0" cellspacing="0"></table>
                            <div id="jqgpclassFeeGroup" style="text-align: center;"></div>
                        </div>
                    </div>


                    <div class="card">
                        <div class="card-header">

                        </div>
                        <div id="attendance-gridContainer" class="card-body">

                            <table id="jqgattendance" cellpadding="0" cellspacing="0"></table>
                            <div id="jqgpattendance" style="text-align: center;"></div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-header">

                        </div>
                        <div id="timetable-gridContainer" class="card-body">
                            <table id="jqgtimetable" cellpadding="0" cellspacing="0"></table>
                            <div id="jqgptimetable" style="text-align: center;"></div>

                        </div>
                    </div>
                </div>
                @*<div class="row" >
                            <div class="col-12 col-md-6">
                            </div>
                            <div class="col-12 col-md-6">
                            </div>
                            <div class="col-12 col-md-6">
                            </div>
                            <div class="col-12 col-md-6">
                            </div>
                            <div class="col-12 col-md-6">
                            </div>
                        </div>*@
            </div>
        </div>
    </div>
</div>
<script>
    function loaded() {
        let grid = document.getElementById("list_attendance").ej2_instances[0];
        grid.dataSource = [];
    }
</script>
<script defer src="/assets/pages/dashboard.js"></script>